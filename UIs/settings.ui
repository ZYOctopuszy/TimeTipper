   <string notr="true">/* 基础设置 */
* {
    font-family: 'Microsoft YaHei', 'Segoe UI', sans-serif;
    color: #e0e0e0;
}

/* 主窗口 */
QWidget#Form {
    background-color: rgba(20, 30, 20, 0.95);
    border: 1px solid rgba(60, 90, 60, 0.7);
    border-radius: 8px;
}

/* 标签页容器 */
QTabWidget::pane {
    border: 1px solid rgba(50, 80, 50, 0.0);
    background: rgba(30, 50, 30, 0.0);
    border-radius: 6px;
    margin: 4px;
}

/* 标签页标签 */
QTabBar::tab {
    background: rgba(40, 60, 40, 0.8);
    border: 1px solid rgba(70, 100, 70, 0.6);
    border-bottom: none;
    color: #b0d0b0;
    padding: 6px 12px;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
    margin-right: 2px;
}

QTabBar::tab:selected {
    background: rgba(60, 90, 60, 0.9);
    color: white;
    border-bottom: 1px solid rgba(60, 90, 60, 0.9);
}

QTabBar::tab:hover {
    background: rgba(80, 110, 80, 0.8);
    color: white;
}

/* 按钮基础样式 */
QPushButton {
    background-color: rgba(50, 120, 60, 0.85);
    color: white;
    border: 1px solid rgba(80, 150, 90, 0.7);
    border-radius: 4px;
    padding: 6px 12px;
    min-width: 80px;
}

QPushButton:hover {
    background-color: rgba(70, 140, 80, 0.9);
    border-color: rgba(100, 170, 110, 0.8);
}

QPushButton:pressed {
    background-color: rgba(30, 100, 40, 0.9);
}

QPushButton:disabled {
    background-color: rgba(46, 125, 50, 0.7);
    color: rgba(200, 230, 200, 0.7);
    border-color: rgba(46, 125, 50, 0.6);
}

/* 特殊按钮样式 */
QPushButton#is_active {
    background-color: rgba(80, 160, 90, 0.85);
    border-color: rgba(110, 190, 120, 0.7);
}

QPushButton#is_active:hover {
    background-color: rgba(100, 180, 110, 0.9);
}

QPushButton#test_button {
    background-color: rgba(120, 60, 50, 0.85);
    border-color: rgba(150, 90, 80, 0.7);
}

QPushButton#test_button:hover {
    background-color: rgba(140, 80, 70, 0.9);
}

/* 列表和文本编辑框 */
QListWidget, QTextEdit {
    background-color: rgba(40, 50, 40, 0.0);
    border: 1px solid rgba(70, 90, 70, 0.7);
    border-radius: 4px;
    padding: 4px;
    color: #e0e0e0;
    selection-background-color: rgba(60, 120, 70, 0.7);
    selection-color: white;
}

/* 复选框 */
QCheckBox {
    background: transparent;
    padding: 2px;
    spacing: 5px;
}

QCheckBox::indicator {
    width: 16px;
    height: 16px;
    border: 1px solid rgba(90, 120, 90, 0.7);
    border-radius: 3px;
    background: rgba(50, 60, 50, 0.8);
}

QCheckBox::indicator:checked {
    background: rgba(60, 130, 70, 0.9);
}

QCheckBox:hover {
    background-color: rgba(70, 80, 70, 0.3);
}

/* 数值输入框 */
QSpinBox {
    background-color: rgba(40, 50, 40, 0.8);
    border: 1px solid rgba(70, 90, 70, 0.7);
    border-radius: 4px;
    padding: 3px;
    color: #e0e0e0;
}

QSpinBox::up-button, QSpinBox::down-button {
    width: 16px;
    border: none;
    background: rgba(60, 80, 60, 0.7);
}

QSpinBox::up-button:hover, QSpinBox::down-button:hover {
    background: rgba(80, 100, 80, 0.8);
}

/* 框架样式 */
QFrame#frame_2, #frame_4{
    background-color: rgba(35, 45, 35, 0.1);
    border: 1px solid rgba(70, 90, 70, 0.6);
    border-radius: 6px;
}
QFrame#frame_3 {
    background-color: rgba(35, 45, 35, 0.8);
    border: 1px solid rgba(70, 90, 70, 0.6);
    border-radius: 6px;
}
QFrame#frame {
    background-color: rgba(35, 45, 35, 0.0);
    border: 1px solid rgba(70, 90, 80 ,0.6);
    border-radius: 6px;
}

/* 标签样式 */
QLabel {
    background: transparent;
}

/* 关于页大标题 */
QLabel#label {
    color: rgba(100, 220, 120, 0.9);
}

/* 分隔线 */
QFrame[frameShape=&quot;4&quot;], QFrame[frameShape=&quot;5&quot;] {
    background-color: rgba(70, 90, 70, 0.0);
}

/* 滚动条 */
QScrollBar:vertical, QScrollBar:horizontal {
    background: rgba(40, 50, 40, 0.5);
    margin: 0px;
}

QScrollBar:vertical {
    width: 10px;
}

QScrollBar:horizontal {
    height: 10px;
}

QScrollBar::handle:vertical, QScrollBar::handle:horizontal {
    background: rgba(80, 120, 80, 0.6);
    border-radius: 5px;
}

QScrollBar::handle:vertical {
    min-height: 20px;
}

QScrollBar::handle:horizontal {
    min-width: 20px;
}

QScrollBar::add-line:vertical,
QScrollBar::sub-line:vertical,
QScrollBar::add-line:horizontal,
QScrollBar::sub-line:horizontal {
    background: none;
    border: none;
}

QScrollBar::add-page:vertical,
QScrollBar::sub-page:vertical,
QScrollBar::add-page:horizontal,
QScrollBar::sub-page:horizontal {
    background: none;
}

/* 工具提示 */
QToolTip {
    background-color: rgba(40, 50, 40, 0.95);
    color: #e0e0e0;
    border: 1px solid rgba(70, 100, 70, 0.8);
    border-radius: 3px;
    padding: 3px;
}

/* 添加按钮(+) */
QPushButton[text=&quot;+&quot;], 
QPushButton[text=&quot;添加&quot;] {
    background-color: rgba(70, 150, 80, 0.9);
}

/* 删除按钮(-) */
QPushButton[text=&quot;-&quot;], 
QPushButton[text=&quot;删除&quot;] {
    background-color: rgba(150, 70, 60, 0.9);
}

/* 应用按钮特殊样式 */
QPushButton[text=&quot;应用(Alt+A)&quot;] {
    background-color: rgba(80, 140, 90, 0.9);
    font-weight: bold;
}

/* 退出按钮特殊样式 */
QPushButton[text=&quot;退出(Ctrl+Q)&quot;] {
    background-color: rgba(120, 60, 50, 0.85);
}</string>
