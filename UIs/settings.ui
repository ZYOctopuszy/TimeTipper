}

/* 表头样式 */
QHeaderView {
    background-color: rgba(40, 60, 40, 0.8);
    border: none;
    border-radius: 0;
}

QHeaderView::section {
    background-color: rgba(50, 70, 50, 0.9);
    color: #b0d0b0;
    padding: 6px;
    border: 1px solid rgba(70, 100, 70, 0.6);
    border-left: none;
    border-top: none;
    font-weight: normal;
}

QHeaderView::section:first {
    border-left: 1px solid rgba(70, 100, 70, 0.6);
}

QHeaderView::section:checked {
    background-color: rgba(60, 90, 60, 0.9);
    color: white;
}

/* 表项样式 */
QTableWidget::item {
    padding: 4px 8px;
    border-bottom: 1px solid rgba(70, 90, 70, 0.3);
}

QTableWidget::item:hover {
    background-color: rgba(60, 80, 60, 0.4);
}

QTableWidget::item:selected {
    background-color: rgba(60, 120, 70, 0.7);
    color: white;
}

/* 交替行颜色 */
QTableWidget::item:alternate {
    background-color: rgba(45, 55, 45, 0.2);
}

/* 角部按钮样式 */
QTableCornerButton::section {
    background-color: rgba(50, 70, 50, 0.9);
    border: 1px solid rgba(70, 100, 70, 0.6);
    border-left: none;
    border-top: none;
}

/* 滚动条样式（与主样式一致） */
QTableWidget QScrollBar:vertical {
    background: rgba(40, 50, 40, 0.5);
    width: 10px;
}

QTableWidget QScrollBar::handle:vertical {
    background: rgba(80, 120, 80, 0.6);
    min-height: 20px;
    border-radius: 5px;
}

QTableWidget QScrollBar::add-line:vertical, 
QTableWidget QScrollBar::sub-line:vertical {
    background: none;
}

/* 编辑状态下的样式 */
